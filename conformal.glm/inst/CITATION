
year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{conformal.glm}: Conformal Prediction for Generalized Linear Regression Models",
         author = c(person("Daniel J.", "Eck"),
                    person("Forrest W.", "Crawford")),
         year = year,
         note = note,
         url = "https://github.com/DEck13/conformal.glm")